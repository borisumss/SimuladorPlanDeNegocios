<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.simuladorplandenegocios">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SimuladorPlanDeNegocios">
        <activity
            android:name=".Vista.VistaPlanNegocioPDF"
            android:exported="false"
            android:parentActivityName=".Vista.MenuSimulacion2"/>

        <activity
            android:name=".Vista.MenuSimulacion2"
            android:exported="false"
            android:parentActivityName=".Vista.VistaPlanNegocioPDF"/>

        <activity
            android:name=".Vista.MenuSimulacion"
            android:exported="false"
            android:parentActivityName=".Vista.MainActivity" />
        <activity
            android:name=".Vista.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Vista.MainActivity"
            android:exported="true" />
    </application>

</manifest>